#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules/next/dist/bin/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules/next/dist/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules/next/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules/next/dist/bin/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules/next/dist/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules/next/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/next@16.0.0_@babel+core@7.28.4_@playwright+test@1.56.1_react-dom@19.2.0_react@19.2.0__react@19.2.0/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
