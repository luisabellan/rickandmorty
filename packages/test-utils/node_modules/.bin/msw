#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/msw@2.11.5_@types+node@24.9.1_typescript@5.9.3/node_modules/msw/cli/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/msw@2.11.5_@types+node@24.9.1_typescript@5.9.3/node_modules/msw/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/msw@2.11.5_@types+node@24.9.1_typescript@5.9.3/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/msw@2.11.5_@types+node@24.9.1_typescript@5.9.3/node_modules/msw/cli/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/msw@2.11.5_@types+node@24.9.1_typescript@5.9.3/node_modules/msw/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/msw@2.11.5_@types+node@24.9.1_typescript@5.9.3/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../msw/cli/index.js" "$@"
else
  exec node  "$basedir/../msw/cli/index.js" "$@"
fi
