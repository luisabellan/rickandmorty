#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/@pandacss+dev@0.45.2_jsdom@20.0.3_typescript@5.9.3/node_modules/@pandacss/dev/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/@pandacss+dev@0.45.2_jsdom@20.0.3_typescript@5.9.3/node_modules/@pandacss/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/@pandacss+dev@0.45.2_jsdom@20.0.3_typescript@5.9.3/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/@pandacss+dev@0.45.2_jsdom@20.0.3_typescript@5.9.3/node_modules/@pandacss/dev/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/@pandacss+dev@0.45.2_jsdom@20.0.3_typescript@5.9.3/node_modules/@pandacss/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/@pandacss+dev@0.45.2_jsdom@20.0.3_typescript@5.9.3/node_modules:/home/squarez/Work/git/current-projects/rickandmorty/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@pandacss/dev/bin.js" "$@"
else
  exec node  "$basedir/../@pandacss/dev/bin.js" "$@"
fi
